<Window x:Class="SCQueryConnect.Views.AttributeMappingEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:SCQueryConnect.ViewModels"
        mc:Ignorable="d"
        Height="450"
        Title="Attribute Mapping Editor"
        Width="800"
        WindowStartupLocation="CenterOwner">

    <Window.Resources>
        <viewModels:ViewModelLocator x:Key="ViewModelLocator" />
    </Window.Resources>

    <Grid Background="{StaticResource QCBackground}"
          DataContext="{Binding AttributeMappingEditorViewModel, Source={StaticResource ViewModelLocator}}">
    </Grid>
</Window>
